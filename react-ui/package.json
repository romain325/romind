{
  "name": "react-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.14.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile-sass": "node-sass src/assets/scss/main.scss src/assets/css/main.css",
    "minify-sass": "node-sass src/assets/scss/main.scss src/assets/css/main.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/main.scss src/assets/css/main.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6"
  }
}
